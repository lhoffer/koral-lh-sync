
<!doctype html>
<html class="no-js supports-no-cookies" lang="{{ shop.locale }}"> 
  <head>
                
{% render 'orbislabs_sitemap_noindex' %}

  
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    {%- if page_description -%}
      <meta name="description" content="{{ page_description | escape }}">
    {%- endif -%}
    {%- include 'head__meta' -%}
    {%- include 'social-meta-tags' -%}
    
    {% include 'user1st-script' %}
  
    <link rel="canonical" href="{{ canonical_url }}">
    {%- if settings.favicon != blank -%}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
    {%- endif -%}
  
    <link rel="stylesheet" href="https://use.typekit.net/vmt3hoy.css">
    {{ 'theme.bundle.css' | asset_url | stylesheet_tag }}
    {{ 'fonts.scss.css' | asset_url | stylesheet_tag }}
  
    {{ 'jquery.min.js' | asset_url | script_tag }}
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/gh/kenwheeler/slick@1.8.1/slick/slick.css"/>
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/gh/kenwheeler/slick@1.8.1/slick/slick-theme.css"/>
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css"/>
    <script type="text/javascript" src="//cdn.jsdelivr.net/gh/kenwheeler/slick@1.8.1/slick/slick.min.js"></script>
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />
    {%- capture seo_title -%}
      {{ page_title }}
      {%- if current_tags -%}
        {%- assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags -}}
      {%- endif -%}
      {%- if current_page != 1 -%}
        &ndash; {{ 'general.meta.page' | t: page: current_page }}
      {%- endif -%}
      {%- unless page_title -%}
        &ndash; {{ shop.name }}
      {%- endunless -%}
    {%- endcapture -%}  
    <title>{{ seo_title }}</title>
  
    {%- if template.directory == 'customers' -%}
      <!--[if (gt IE 9)|!(IE)]><!--><script src="{{ 'shopify_common.js' | shopify_asset_url }}" defer="defer"></script><!--<![endif]-->
      <!--[if lte IE 9]><script src="{{ 'shopify_common.js' | shopify_asset_url }}"></script><![endif]-->
    {%- endif -%}
  
    {{ content_for_header }}
    {% comment %} {%- include 'bold-product' with product, hide_action: 'header' -%} {% endcomment %}
    {% comment %} {%- include 'bold-product' with product, hide_action: 'header' -%} {% endcomment %}
    {%- render 'bold-pr' -%}
    {%- render 'bold-common' -%}
    {{ 'bold-custom.css' | asset_url | stylesheet_tag }}
    {%- if settings.collection_use_filters -%}
    <!--begin-bc-sf-filter-css-->
      {% include 'bc-sf-filter-style' %}
    <!--end-bc-sf-filter-css-->
    {%- endif -%}
    {% include 'shogun-head' %}
  
    {% include 'magic-links' %}
  
    {% include 'global-e-head' %}
    
